{
	"name": "tas2580/imageproxy",
	"type": "phpbb-extension",
	"description": "Displays every image over a local file, which is included with [img] bbcode and not hosted with the own board.",
	"homepage": "https://tas2580.net/downloads/phpbb-imageproxy/",
	"version": "0.1.0",
	"time": "2015-12-02",
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "tas2580",
			"homepage": "https://tas2580.net",
			"email": "mail@tas2580.net",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"extra": {
		"display-name": "Image Proxy",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.3"
		},
		"version-check": {
			"host": "phpbb-versionscheck.cf",
			"directory": "/tas2580/imageproxy/master",
			"filename": "version-check.json"
		}
	}
}
